<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Khi mục được chọn -->
    <item android:color="@color/color_selected" android:state_checked="true"
        android:state_activated="false"/>
    <!-- Khi mục không được chọn -->
    <item android:color="@color/color_unselected" android:state_checked="false"/>
</selector>
